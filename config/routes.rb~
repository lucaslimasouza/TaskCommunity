TaskCommunity::Application.routes.draw do

  devise_for :users

  resources :home, :only => :index
  #resources :lists

  resources :watches, :only => [:index, :destroy] do
	  get 'add',:on => :member
	end

  resources :users  { |users| users.resources :lists, :controller => "users/lists"}

  root :to => "home#index"

end
